<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InputLayout">
        <attr name="android:inputType" format="reference" />
        <attr name="android:nextFocusDown" format="reference|integer" />
        <attr name="android:hint" format="reference|string" />
        <attr name="iconSrc" format="reference" />
        <attr name="layoutInputType" format="enum">
            <enum name="none" value="0" />
            <enum name="email" value="1" />
            <enum name="password" value="2" />
            <enum name="text" value="3" />
        </attr>
        <attr name="minLength" format="integer" />
        <attr name="maxLength" format="integer" />
    </declare-styleable>
</resources>